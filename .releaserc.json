{
    "tagFormat": "v${version}",
    "plugins": [
        ["@semantic-release/commit-analyzer", {
            "releaseRules": [
                {
                    "type": "milestone",
                    "release": "major"
                },
                {
                    "type": "feat",
                    "release": "minor"
                },
                {
                    "type": "change",
                    "release": "patch"
                },
                {
                    "type": "fix",
                    "release": "patch"
                },
                {
                    "type": "obsolete",
                    "release": false
                },
                {
                    "type": "remove",
                    "release": false
                },
                {
                    "type": "ci",
                    "release": false
                },
                {
                    "type": "docs",
                    "release": false
                },
                {
                    "type": "build",
                    "release": false
                },
                {
                    "type": "chore",
                    "release": false
                }
            ]
        }],
        ["@semantic-release/release-notes-generator", {
            "preset": "angular",
            "presetConfig": {
                "types": [
                    {
                        "type": "build",
                        "section": "üì∞ Notices",
                        "hidden": false
                    },
                    {
                        "type": "milestone",
                        "section": "üöÄ Features",
                        "hidden": false
                    },
                    {
                        "type": "feat",
                        "section": "‚ú® Added",
                        "hidden": false
                    },
                    {
                        "type": "change",
                        "section": "üîÑ Changed",
                        "hidden": false
                    },
                    {
                        "type": "fix",
                        "section": "üîß Fixed",
                        "hidden": false
                    },
                    {
                        "type": "obsolete",
                        "section": "‚ö†Ô∏è Deprecated",
                        "hidden": false
                    },
                    {
                        "type": "remove",
                        "section": "üóëÔ∏è Removed",
                        "hidden": false
                    },
                    {
                        "type": "docs",
                        "section": "üìö Documentation",
                        "hidden": false
                    },
                    {
                        "type": "ci",
                        "section": "üßë‚Äçüíª Continuous Integration",
                        "hidden": true
                    },
                    {
                        "type": "chore",
                        "section": "‚úîÔ∏è Tasks",
                        "hidden": true
                    }
                ]
            }
        }],
        ["@semantic-release/npm", { "npmPublish": false }],
        ["@semantic-release/git", {
            "assets": ["package.json", "CHANGELOG.md"],
            "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }],
        "@semantic-release/github"
    ]
}